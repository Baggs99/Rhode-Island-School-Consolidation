{
  "name": "rhode-island-school-consolidation-map",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:data": "tsx scripts/build-data.ts",
    "build:districts": "tsx scripts/build-districts.ts",
    "build:public": "tsx scripts/build-public-schools.ts",
    "build:private": "tsx scripts/build-private-schools.ts",
    "dev": "concurrently -n backend,frontend -c blue,green \"npm run dev --workspace backend\" \"npm run dev --workspace frontend\"",
    "build": "npm run build --workspace frontend",
    "lint": "npm run lint --workspace frontend"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "dependencies": {
    "@turf/boolean-point-in-polygon": "^7.1.0",
    "@turf/helpers": "^7.1.0",
    "adm-zip": "^0.5.16",
    "csv-parse": "^5.5.6",
    "shapefile": "^0.6.6",
    "shpjs": "^6.1.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "tsx": "^4.7.0",
    "typescript": "^5.7.0"
  }
}
